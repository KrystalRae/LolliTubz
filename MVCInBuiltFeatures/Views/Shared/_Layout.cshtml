<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Lollitubz</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Application to order lollitubz stock easily." name="description" />
    <meta content="Krystal Randall" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="/assets/images/favicon.ico">

    <!-- App css -->
    <link href="/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="/assets/css/app.css" rel="stylesheet" type="text/css" />

</head>

<body class="authentication-bg authentication-bg-pattern main">
    <div id="wrapper">
        <div class="navbar-custom">
            <ul class="list-unstyled topnav-menu float-left mb-0">
                <li>
                    <a href="javascript:history.back()" class="nav-link" style="font-size: x-large;padding-left: 10px;padding-right: 0;">
                        <i class="fe-arrow-left"></i>
                    </a>
                </li>
            </ul>
            <div class="logo-box">
                <a href="/" class="logo text-center">
                    <span class="logo-lg">
                        <img src="~/assets/images/Lolli_Tubz_Logo-min.jpg" alt="" class="header-logo">
                        Lollitubz
                    </span>

                </a>
            </div>
            <ul class="list-unstyled topnav-menu float-right mb-0">
                <li>
                    <a href="@Url.Action("LogOff", "Account")" class="nav-link" style="font-size: x-large;">
                        <i class="fe-log-out"></i>
                    </a>
                </li>
            </ul>
        </div>

        <div class="content-page">
            <div class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
            <footer class="footer footer-alt">
                @DateTime.Now.Year &copy; by Krystal Randall
            </footer>

        </div>
    </div>
    <div class="rightbar-overlay"></div>

    <!-- Vendor js -->
    <script src="/assets/js/vendor.min.js"></script>

    <!-- App js -->
    <script src="/assets/js/app.min.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>